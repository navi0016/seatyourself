<h1>Log in</h1>

<%= form_for :session, url: sessions_path do |f| %>

  <div class="log-in email">
  <%= f.label :email %>
  <%= f.text_field :email %>
  </div>

  <div class="log-in password">
  <%= f.label :password %>
  <%= f.password_field :password %>
  </div>

  <%= f.submit "Log in" %>

<% end %>
